<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="anchorPane" prefHeight="364.0" prefWidth="562.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.karalius.attendance.ModifyingGroupsController">
   <children>
      <TextField fx:id="newName" layoutX="14.0" layoutY="98.0" promptText="enter group name to add" />
      <Button layoutX="14.0" layoutY="157.0" mnemonicParsing="false" onAction="#delete" text="Delete selected group" />
      <Button layoutX="171.0" layoutY="98.0" mnemonicParsing="false" onAction="#add" text="Add" />
      <Button layoutX="14.0" layoutY="325.0" mnemonicParsing="false" onAction="#backToMainWindow" text="Back to main window" />
   </children>
</AnchorPane>
